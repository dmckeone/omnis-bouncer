{
  "name": "omnis-bouncer-ui",
  "version": "0.1.2",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "cross-env VITE_API_URI=https://127.0.0.1:2999/ vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test": "vitest watch",
    "test-ci": "vitest run --reporter=junit --outputFile=./vitest.xml",
    "build-only": "cross-env VITE_API_URI=/ vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "lint-ci": "eslint .",
    "format": "prettier --write src/",
    "format-ci": "prettier --check src/",
    "story:dev": "histoire dev",
    "story:build": "histoire build",
    "story:build-ci": "histoire build -c histoire-ci.config.ts"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.14",
    "@vueuse/core": "^13.9.0",
    "pinia": "^3.0.3",
    "tailwindcss": "^4.1.14",
    "vitest-fetch-mock": "^0.4.5",
    "vue": "^3.5.22",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@histoire/plugin-vue": "1.0.0-alpha.3",
    "@pinia/testing": "^1.0.2",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@tsconfig/node22": "^22.0.2",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.18.8",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/eslint-plugin": "^1.3.13",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "cross-env": "^10.1.0",
    "daisyui": "^5.1.26",
    "eslint": "^9.36.0",
    "eslint-plugin-vue": "~10.3.0",
    "eventsourcemock": "^2.0.0",
    "histoire": "1.0.0-alpha.3",
    "jiti": "^2.6.1",
    "jsdom": "^26.1.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.6.2",
    "typescript": "~5.8.3",
    "vite": "^7.1.8",
    "vite-plugin-vue-devtools": "^8.0.2",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.1.0"
  }
}
